{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "ApplicationName": "Bico API"
    }
  },

  "AllowedHosts": "*",

  "AzureKeyVault": {
    "Endpoint": "https://bico-vault.vault.azure.net/"
  }

}
